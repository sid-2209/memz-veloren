root   ::= "{" ws "\"reflection\"" ws ":" ws string "," ws "\"new_beliefs\"" ws ":" ws string-array "," ws "\"questions\"" ws ":" ws string-array "," ws "\"mood_shift\"" ws ":" ws mood-obj "}" ws

string       ::= "\"" ([^"\\] | "\\" .)* "\""
string-array ::= "[" ws (string (ws "," ws string)*)? ws "]"
mood-obj     ::= "{" ws "\"pleasure\"" ws ":" ws number "," ws "\"arousal\"" ws ":" ws number "," ws "\"dominance\"" ws ":" ws number ws "}"
number       ::= "-"? ("0" | [1-9] [0-9]*) ("." [0-9]{1,2})?
ws           ::= [ \t\n]*
